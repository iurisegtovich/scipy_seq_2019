   "source": [
    "from matplotlib import pyplot as plt\n",
    "%matplotlib inline \n",
    "\n",
    "import numpy as np\n",
    "\n",
    "from scipy.integrate import odeint\n",
    "\n",
    "def f(y,t):\n",
    "    return -2*y\n",
    "\n",
    "t = np.linspace(-1, 10, 100)\n",
    "y = odeint(f, 1, t)\n",
    "plt.scatter(-1,1)\n",
    "plt.plot(t,y)"
   ]
